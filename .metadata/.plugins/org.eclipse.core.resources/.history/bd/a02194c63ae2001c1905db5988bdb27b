package tree;

public class BinarySearchTree {
	Node root;
	public void Insert(int insertKey) {
		Node newNode = new Node(insertKey);
		root = newNode;
		return;
	}
	public void Insert(int parentKey, int insertKey) {
		Node newNode = new Node(insertKey);
		Node focusNode = root;
		while(true) {
			if (parentKey < focusNode.key) {
				focusNode = focusNode.left;
			}else if (parentKey > focusNode.key) {
				focusNode = focusNode.right;
			}else {
				if (insertKey < parentKey) {
					focusNode.left = newNode;
					return;
				}else {
					focusNode.right = newNode;
					return;
				}
			}
		}
	}
	
}

class Node{
	int key;
	Node left, right;
	Node(int key){
		this.key = key;
	}
}
